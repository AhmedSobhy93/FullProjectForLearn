<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools" android:layout_width="match_parent"
    android:layout_height="match_parent" android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    android:paddingBottom="@dimen/activity_vertical_margin"
    tools:context="com.example.ahmed.calculator.List_View">


    <ListView
        android:layout_width="fill_parent"
        android:layout_height="100dp"
        android:id="@+id/listView"

        android:layout_below="@+id/Main_Numbers_TextView"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />
    <!--   android:entries="@array/test" -->
    <!--  android:entries="@array/myList" -->
    <ListView
        android:layout_width="fill_parent"
        android:layout_height="100dp"
        android:id="@+id/SecondPage_MyList_ListView"

        android:layout_below="@+id/Main_Names_TextView"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true" />
    <TextView
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceLarge"
        android:text="Names"
        android:id="@+id/Main_Names_TextView"
        android:layout_alignParentTop="true"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true"
        android:layout_marginTop="63dp" />

    <TextView
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceLarge"
        android:text="Numbers"
        android:id="@+id/Main_Numbers_TextView"
        android:layout_centerVertical="true"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true" />


</RelativeLayout>
